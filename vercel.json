{
  "version": 2,
  "builds": [
    {
      "src": "Server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/(.*)",
      "dest": "Server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
